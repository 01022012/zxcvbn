{
  "name": "zxcvbn",
  "version": "2.0.2",
  "description": "realistic password strength estimation",
  "main": "zxcvbn.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/dropbox/zxcvbn.git"
  },
  "scripts": {
    "build": "browserify --standalone zxcvbn -t coffeeify --extension='.coffee' -t uglifyify main.coffee -o zxcvbn.js",
    "watch": "watchify -v --standalone zxcvbn -t coffeeify --extension='.coffee' -t uglifyify main.coffee -o zxcvbn.js"
  },
  "keywords": [
    "realistic",
    "password",
    "strength",
    "estimation",
    "estimate",
    "meter",
    "validate",
    "validation"
  ],
  "author": "Dan Wheeler",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^11.0.1",
    "coffeeify": "^1.1.0",
    "mocha": "^2.2.5",
    "uglifyify": "^3.0.1",
    "watchify": "^3.3.1"
  }
}
